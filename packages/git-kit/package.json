{
  "name": "@toolbox-ts/git-kit",
  "version": "0.0.0",
  "private": false,
  "description": "Git Kit CLI for managing standardized Git operations",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gajdascz/toolbox-ts"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "dependency-cruiser",
    "typed-api",
    "cli",
    "dependency",
    "graph",
    "validation"
  ],
  "author": {
    "name": "Nolan Gajdascz",
    "email": "dev.gajdascz@gmail.com",
    "url": "https://github.com/gajdascz"
  },
  "type": "module",
  "scripts": {},
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "@oclif/core": "^4.5.2",
    "@toolbox-ts/cli-kit": "workspace:^",
    "@toolbox-ts/file": "workspace:^",
    "@toolbox-ts/utils": "workspace:^",
    "dependency-cruiser": "^16.10.4"
  },
  "devDependencies": {
    "@types/node": "^18.19.123",
    "tsx": "^4.20.5"
  },
  "bin": {
    "git-kit": "./dist/bin/run.js",
    "gitkit": "./dist/bin/run.js"
  },
  "oclif": {
    "bin": "git-kit",
    "commands": {
      "target": "./dist/core/commands",
      "strategy": "pattern"
    },
    "dirname": "git-kit",
    "topicSeparator": " "
  }
}
