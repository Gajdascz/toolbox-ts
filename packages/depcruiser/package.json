{
  "name": "@toolbox-ts/depcruiser",
  "version": "0.1.1",
  "description": "Dependency cruiser utilities.",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/gajdascz/toolbox-ts/tree/main/packages/depcruiser",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajdascz/toolbox-ts"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "dependency-cruiser",
    "dependency-graph",
    "toolbox-ts",
    "utilities",
    "cli",
    "api"
  ],
  "author": {
    "name": "Nolan Gajdascz",
    "email": "dev.gajdascz@gmail.com",
    "url": "https://github.com/gajdascz"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.js",
      "types": "./dist/api/index.d.ts"
    },
    "./definitions": {
      "import": "./dist/definitions/index.js",
      "types": "./dist/definitions/index.d.ts"
    }
  },
  "dependencies": {
    "@oclif/core": "^4.5.4",
    "@toolbox-ts/cli-kit": "workspace:^",
    "@toolbox-ts/file": "workspace:^",
    "@toolbox-ts/utils": "workspace:^",
    "dependency-cruiser": "^16.10.4",
    "watskeburt": "^4.2.3"
  },
  "devDependencies": {
    "@types/node": "^18.19.127",
    "tsx": "^4.20.5"
  },
  "bin": {
    "depcruiser": "./dist/cli/bin/run.js"
  },
  "oclif": {
    "bin": "depcruiser",
    "commands": {
      "target": "./dist/cli/command/depcruiser.js",
      "strategy": "single"
    },
    "dirname": "depcruiser",
    "topicSeparator": " "
  }
}
